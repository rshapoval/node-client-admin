<header class="flex-shrink-0">
  <nav class="navbar navbar-expand-lg bg-light">
    <div class="container">
      <a class="navbar-brand" href="/"><%= sharedData.logo %></a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
          <% if (typeof sharedData.nav !== 'undefined' && sharedData.nav.length) { %>
            <% sharedData.nav.forEach(({ text, url }, index) => { %>
              <li class="nav-item">
                <a class="nav-link" href="<%= url %>"><%= text %></a>
              </li>
            <% }) %>
            <li class="nav-item">
              <span class="nav-link">|</span>
            </li>
            <% if (typeof sharedData.user !== 'undefined' && sharedData.user) { %>
              <li class="nav-item">
                <a class="nav-link" href="/dashboard">Dashboard</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/logout">Logout</a>
              </li>
            <% } else { %>    
              <li class="nav-item">
                <a class="nav-link" href="/login">Login</a>
              </li>
            <% }%>    
          <% } %>
        </ul>
      </div>
    </div>
  </nav>
</header>